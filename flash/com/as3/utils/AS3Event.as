package com.as3.utils{	import flash.events.Event;	public class AS3Event extends Event	{		public static const BUTTON_CLICK = "btn_click";		public static const BUTTON_OVERCLICK = "btn_ovrclick";		public static const BUTTON_DOUBLECLICK = "btn_dclick";		public static const SLIDER_SEEK = "slider_seek";		public static const SLIDER_PREVIEW = "slider_preview";		public static const TIMEOUT = "timeout";        private var _data_obj:Object;		public function AS3Event(evt:String,id:*=null,info:*=null)		{			super(evt);			if (id != null || info != null)				_data_obj={id:id,info:info};		}		public function set obj(o:Object):void		{			_data_obj=o;		}		public function get obj():Object		{			return _data_obj;		}	}}